#This file generated via template by Chef.
<% @apache_listen_ports.each do |port| -%>
<% if port == '443' %>
<IfModule mod_ssl.c>
  Listen <%= port %>
  # SSL name based virtual hosts are not yet supported, therefore no
  # NameVirtualHost statement here
  #NameVirtualHost *:<%= port %>
</IfModule>
<% else %>
Listen <%= port %>
NameVirtualHost *:<%= port %>
<% end -%>
<% end -%>
